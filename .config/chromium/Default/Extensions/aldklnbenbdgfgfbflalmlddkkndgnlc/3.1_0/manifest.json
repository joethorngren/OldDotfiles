{
   "author": "Natalie Perna",
   "background": {
      "scripts": [ "background.js" ]
   },
   "commands": {
      "_execute_page_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+L",
            "mac": "MacCtrl+Shift+L"
         }
      }
   },
   "description": "Allow Trello lists to be displayed vertically or in a grid.",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "List Layouts for Trello",
   "page_action": {
      "default_title": "List Layouts for Trello"
   },
   "permissions": [ "*://trello.com/b/*", "tabs", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.1",
   "web_accessible_resources": [ "css/layout.css" ]
}
